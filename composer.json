{
  "name": "yaangvu/laravel-base",
  "description": "Laravel API Resource Base",
  "keywords": [
    "laravel",
    "lumen",
    "base",
    "yaangvu",
    "api",
    "generator",
    "swagger"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1|^8.2",
    "ramsey/uuid": "^4.7.1"
  },
  "require-dev": {
    "illuminate/database": "^7|^8|^9|^10",
    "illuminate/pagination": "^7|^8|^9|^10",
    "illuminate/http": "^7|^8|^9|^10",
    "illuminate/console": "^7|^8|^9|^10"
  },
  "authors": [
    {
      "name": "Yaang Vu",
      "email": "yaangvu@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "YaangVu\\LaravelBase\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YaangVu\\LaravelBase\\Providers\\BaseServiceProvider"
      ],
      "aliases": {}
    }
  }
}

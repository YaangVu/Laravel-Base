{
  "name": "yaangvu/laravel-base",
  "description": "Laravel YaangVu Base",
  "keywords": [
    "laravel",
    "lumen",
    "base",
    "yaangvu",
    "api"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/database": "^7|^8|^9",
    "illuminate/pagination": "^7|^8|^9",
    "illuminate/http": "^7|^8|^9",
    "illuminate/console": "^7|^8|^9",
    "yaangvu/laravel-exception": "^1.0",
    "fakerphp/faker": "1.17.x-dev"
  },
  "authors": [
    {
      "name": "Yaang Vu",
      "email": "yaangvu@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "YaangVu\\LaravelBase\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YaangVu\\LaravelBase\\Providers\\BaseServiceProvider"
      ],
      "aliases": {
        "BaseService": "YaangVu\\LaravelBase\\Services\\BaseService",
        "Query": "YaangVu\\LaravelBase\\Facades\\Query"
      }
    }
  }
}
